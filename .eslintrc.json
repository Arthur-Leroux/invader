{
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "extends": "eslint:recommended",
    "rules": {
      "camelcase": 2,
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "backticks"
      ]
    },
    "env": {
      "browser": true
    }
  }